    {
      "type": "java",
      "name": "1 Config Service",
      "request": "launch",
      "mainClass": "com.saigon.services.application.ConfigServiceApplication",
      "projectName": "config-service"
    },
    {
      "type": "java",
      "name": "2A Discovery Service",
      "request": "launch",
      "mainClass": "com.saigon.services.application.DiscoveryServiceApplication",
      "projectName": "discovery-service"
    },
    {
      "type": "java",
      "name": "2B Discovery Service",
      "request": "launch",
      "mainClass": "com.saigon.services.application.DiscoveryServiceApplication",
      "projectName": "discovery-service",
      "args": "--spring.profiles.active=localhost"
    },
    {
      "type": "java",
      "name": "2C Discovery Service",
      "request": "launch",
      "mainClass": "com.saigon.services.application.DiscoveryServiceApplication",
      "projectName": "discovery-service",
      "args": "--spring.profiles.active=development"
    },
    {
      "type": "java",
      "name": "2D Discovery Service",
      "request": "launch",
      "mainClass": "com.saigon.services.application.DiscoveryServiceApplication",
      "projectName": "discovery-service",
      "args": "--spring.profiles.active=production"
    },
    {
      "type": "java",
      "name": "3A Gateway Service",
      "request": "launch",
      "mainClass": "com.saigon.services.application.GatewayServiceApplication",
      "projectName": "gateway-service"
    },
    {
      "type": "java",
      "name": "3B Gateway Service",
      "request": "launch",
