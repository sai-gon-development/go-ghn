      "args": "--spring.profiles.active=localhost"
    },
    {
      "type": "java",
      "name": "3C Gateway Service",
      "request": "launch",
      "mainClass": "com.saigon.services.application.GatewayServiceApplication",
      "projectName": "gateway-service",
      "args": "--spring.profiles.active=development"
    },
    {
      "type": "java",
      "name": "3D Gateway Service",
      "request": "launch",
      "mainClass": "com.saigon.services.application.GatewayServiceApplication",
